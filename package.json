{
  "name": "type-r",
  "version": "1.1.0-a00",
  "description": "Reactive serializable data layer for modern JS applications",
  "main": "./dist/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "prepublish": "node_modules/.bin/webpack -p",
    "test": "node_modules/.bin/mocha tests/nestedtypes/functionality && npm run test:ts",
    "test:ts": "node_modules/.bin/mocha --opts tests/typescript/mocha.opts",
    "build": "node_modules/.bin/tsc && npm run prepublish",
    "docs": "node docs/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gaperton/TransactionalJS.git"
  },
  "keywords": [
    "model",
    "backbone",
    "data",
    "nestedtypes",
    "schema",
    "serialization"
  ],
  "author": "Vlad Balin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gaperton/TransactionalJS/issues"
  },
  "homepage": "https://github.com/gaperton/TransactionalJS#readme",
  "devDependencies": {
    "@types/chai": "^4.0.0",
    "@types/mocha": "^2.2.41",
    "chai": "^4.0.2",
    "handlebars": "^4.0.10",
    "highlight.js": "^9.12.0",
    "marked": "^0.3.6",
    "mocha": "^3.4.2",
    "source-map-loader": "*",
    "ts-loader": "*",
    "ts-node": "^3.0.6",
    "typescript": "*",
    "webpack": "*"
  }
}
